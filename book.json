{
  "title": "Kubernetes Learning",
  "author": "lts0609",
  "language": "zh-hans",
  "structure": {
    "summary": "SUMMARY.md",
    "readme": "README.md"
  },
  "output": {
    "name": "website"
  },
  "plugins": [
    "-highlight",
    "-lunr",
    "-search",
    "-sharing",
    "-sharing-plus",
    "page-treeview",
    "code",
    "chapter-fold",
    "search-pro",
    "popup",
    "scroll-to-top",
    "auto-scroll-table",
    "anchor-navigation-ex",
    "prism",
    "theme-comscore",
    "tbfed-pagefooter",
    "change_girls"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "tocLevel1Icon": "fa fa-cog",
      "tocLevel2Icon": "fa fa-cog",
      "tocLevel3Icon": "fa fa-cog",
      "multipleH1": false,
      "multipleH2": false,
      "multipleH3": false,
      "multipleH4": false,
      "showLevelIcon": false,
      "showLevel": true
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ],
      "lang": {
        "go": "go",
        "yaml": "yaml",
        "shell": "shell"
      }
    },
    "search-pro": {
      "multilingual": false,
      "indexing": {
        "delay": 500,
        "minLength": 2
      }
    },
    "tbfed-pagefooter": {
      "copyright": "© 2025 lts0609. ",
      "modify_label": "最后更新时间:",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "page-treeview": {
      "copyright": "",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "change_girls": {
      "time" : 5,
      "urls" : [
        "https://wallpaperaccess.com/full/8173067.jpg", "https://wallpaperaccess.com/full/6129736.jpg"
      ]
    }
  }
}